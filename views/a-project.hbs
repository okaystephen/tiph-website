<!-- CAROUSEL -->
<div class="container-fluid px-5 pt-5 pb-3 center proj-caro">
  <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
      <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active center">
        <img class="d-block w-100" src="assets/img/test.jpg" alt="First slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="assets/img/test.jpg" alt="Second slide">
      </div>
      <div class="carousel-item">
        <img class="d-block w-100" src="assets/img/test.jpg" alt="Third slide">
      </div>
    </div>
  </div>
  <a class="carousel-control-prev prev-next" href="#carouselExampleIndicators" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next prev-next" href="#carouselExampleIndicators" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

<!-- HEADER -->
<section>
  <div class="container-fluid center">
    <div class="header-title text-center">
      <h5 class="font-weight-bold text-uppercase sub-title">{{this.proj_title}}</h5>
      <p class="sub-caption text-center" id="">{{this.proj_status}} â€¢ {{dateFormat this.proj_date "MMMM Do, YYYY"}}</p>
    </div>
  </div>
</section>

<!-- BODY -->
<div class="the-content sub-caption">
  <!-- CONTENT -->
  {{{this.proj_content}}}
</div>
<br>